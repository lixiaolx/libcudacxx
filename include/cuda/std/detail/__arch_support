#define _LIBCUDACXX_CUDA_ARCH_SM35 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL == _LIBCUDACXX_CUDA_ARCH_VAL_SM35))
