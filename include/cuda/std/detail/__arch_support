#define _LIBCUDACXX_CUDA_ARCH_GREATER_THAN_SM53 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL > _LIBCUDACXX_CUDA_ARCH_VAL_SM53))
#define _LIBCUDACXX_CUDA_ARCH_GREATER_THAN_SM60 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL > _LIBCUDACXX_CUDA_ARCH_VAL_SM60))
#define _LIBCUDACXX_CUDA_ARCH_GREATER_THAN_SM61 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL > _LIBCUDACXX_CUDA_ARCH_VAL_SM61))
#define _LIBCUDACXX_CUDA_ARCH_GREATER_THAN_SM62 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL > _LIBCUDACXX_CUDA_ARCH_VAL_SM62))
#define _LIBCUDACXX_CUDA_ARCH_GREATER_THAN_SM70 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL > _LIBCUDACXX_CUDA_ARCH_VAL_SM70))
