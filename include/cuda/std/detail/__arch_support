#define _LIBCUDACXX_CUDA_ARCH_LESS_THAN_SM61 (_LIBCUDACXX_DEVICE_CHECK(_LIBCUDACXX_CUDA_ARCH_VAL < _LIBCUDACXX_CUDA_ARCH_VAL_SM61))
